  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "rmdir": "node -e \"var fs = require('fs');process.argv.slice(1).map((fpath)=>{ try {fs.rmdirSync(fpath,{recursive: true})} catch(e){console.warn('Error folder:', fpath, e)}});process.exit(0);\"",
    "build:clean": "npm run rmdir --silent -- dist",
    "build:preview": "npm run build && npm run preview",
    "build:search": "node scripts/search-indexer/index.js",
    "link:repo-code": "npm run start --prefix scripts/symlink-repo"
  },
  "dependencies": {
    "@astrojs/rss": "^1.0.0",
    "@astrojs/sitemap": "^1.0.0",
    "astro": "^1.1.3",
    "autoprefixer": "^10.4.8",
    "dayjs": "^1.11.5",
    "lunr": "^2.3.9",
    "sitemap": "^7.1.1"
  },
  "devDependencies": {
    "@astrojs/image": "^0.7.0",
    "@types/node": "^17.0.45",
    "globby": "^13.1.2",
    "mark.js": "^8.11.1",
    "sass": "^1.54.8",
    "tailwindcss": "^3.1.8"
  }
}